generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id       String   @id @default(uuid())
  login    String   @unique
  password String
  FIO      String
  tel      String
  email    String
  role     String   @default("User")
  tokens   Tokens[]
}

model Tokens {
  id     String @id @default(uuid())
  userId String
  user   Users  @relation(fields: [userId], references: [id])
  token  String
}
